{
  "name": "@latouche/osmosis-info-ui",
  "version": "1.0.2",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/esm/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "files": [
    "lib"
  ],
  "type": "module",
  "scripts": {
    "clean": "echo \"CLEAN\" && rm -rf ./node_modules && rm -rf ./lib",
    "build": "rm -rf ./lib && yarn build:esm && yarn build:cjs && yarn build:css",
    "build:css": "yarn tailwindcss -i ./src/styles/globals.css -o ./lib/styles.css --minify",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "build:esm": "tsc",
    "dev:css": "yarn tailwindcss -i ./src/styles/globals.css -o ./lib/styles.css --watch",
    "publish": "yarn build",
    "dev": "tsc -w",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "dependencies": {
    "@use-gesture/react": "^10.2.24",
    "@visx/axis": "^2.18.0",
    "@visx/clip-path": "^2.17.0",
    "@visx/event": "^2.17.0",
    "@visx/gradient": "^2.17.0",
    "@visx/grid": "^2.18.0",
    "@visx/group": "^2.17.0",
    "@visx/mock-data": "^2.17.0",
    "@visx/responsive": "^2.17.0",
    "@visx/scale": "^2.18.0",
    "@visx/shape": "^2.18.0",
    "@visx/tooltip": "^2.17.0",
    "@visx/zoom": "^2.17.0",
    "d3-array": "^3.2.2",
    "d3-time-format": "^4.1.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/d3-array": "3.0.4",
    "@types/d3-time-format": "^4.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "autoprefixer": "^10.4.13",
    "jest": "^29.5.0",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.3"
  }
}